package cosas.de.la.escuela;


public class Motor extends Chasis{
    private int revoluciones;
    private int potencia;
    

    public Motor (int resistencia, String tama単o, String volante){
        super(resistencia, tama単o, volante);
        
        
     
    }

    public Motor(int revoluciones, int potencia, int resistencia, String tama単o, String volante) {
        super(resistencia, tama単o, volante);
        this.revoluciones = revoluciones;
        this.potencia = potencia;
    
    }

    public int getRevoluciones() {
        return revoluciones;
    }

    public int getPotencia() {
        return potencia;
    }
    }
